{
  "parser": "babel-eslint",
  "extends": ["eslint:recommended", "google"],
  "env": {
    "node": true,
    "browser": true,
    "jquery": true,
    "moment": true,
    "es6": true
  },
  "globals": {
    "SP": true,
    "Spotify": true,
    "Handlebars": true
  },
  "rules": {
    "require-jsdoc": 0,
    "max-len": 0,
    "camelcase": 0
  }
}
